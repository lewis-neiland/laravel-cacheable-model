{
    "name": "lewis-neiland/laravel-cacheable-model",
    "description": "A package to manage caching Eloquent models and their relationships.",
    "type": "library",

    "autoload": {
        "psr-4": {
            "LewisNeiland\\Models\\Traits\\": "/src/Models/Traits/"
        }
    },

    "require": {
        "php": ">=7.4",
        "illuminate/support": ">=8.0.0",
        "illuminate/database": ">=8.0.0",
        "illuminate/cache": ">=8.0.0"
    },

    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.1.1"
}